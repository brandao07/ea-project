<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="eaproject.dao.Result" table="Result" lazy="false">
		<id name="ResultId" column="ResultId" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="trial" cascade="lock" column="TrialTrialId" class="eaproject.dao.Trial" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="Position" column="Position" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="Time" column="Time" type="timestamp" not-null="false" lazy="false"/>
		<property name="Observations" column="Observations" type="string" length="255" not-null="false" lazy="false"/>
		<property name="PenaltyTime" column="PenaltyTime" type="timestamp" not-null="false" lazy="false"/>
		<property name="CreationDate" column="CreationDate" type="timestamp" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
