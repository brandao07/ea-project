<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="dao.State" table="State" lazy="false">
		<id name="StateId" column="StateId" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="Nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<property name="CreationDate" column="CreationDate" type="timestamp" not-null="false" lazy="false"/>
		<set name="ORM_Trial" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="StateStateId" not-null="true"/>
			<one-to-many class="dao.Trial"/>
		</set>
	</class>
</hibernate-mapping>
